{
  "expo": {
    "name": "QuickPick",
    "slug": "quickpick",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "quickpick",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yogitha1.quickpick",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["quickpick"]
          }
        ]
      }
    },

    "android": {
      "package": "com.yogitha1.quickpick",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "quickpick"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },

    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "clerkPublishableKey": "pk_test_cmVmaW5lZC1zd2lmdC0xOC5jbGVyay5hY2NvdW50cy5kZXYk"
    }
  }
}